{
  "manifest_version": 2,
  "name": "Lounasjuna",
  "description": "A chrome extension to see who is going where at https://www.lounaat.info/",
  "version": "0.0.1",
  "background": {
    "scripts": [
      "event.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Lounasjuna",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.lounaat.info/*"],
      "css": [],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["activeTab", "https://www.lounaat.info/*"]
}
